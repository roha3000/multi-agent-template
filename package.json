{
  "name": "multi-agent-framework",
  "version": "1.0.0",
  "description": "Multi-agent development system with prompt traceability and token optimization",
  "main": ".claude/core/session-init.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "validate": "node scripts/validate-system.js",
    "traceability": "node scripts/traceability-query.js",
    "cli": "node scripts/cli.js",
    "demo": "node examples/orchestration-demo.js"
  },
  "keywords": [
    "multi-agent",
    "prompt-engineering",
    "traceability",
    "token-optimization"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "chalk": "^4.1.2",
    "inquirer": "^8.2.7",
    "ora": "^5.4.1",
    "tiktoken": "^1.0.22",
    "winston": "^3.18.3"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      ".claude/core/**/*.js",
      "scripts/**/*.js",
      "!scripts/validate-traceability.js",
      "!**/__tests__/**"
    ],
    "testMatch": [
      "**/__tests__/**/*.test.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 30,
        "functions": 30,
        "lines": 30,
        "statements": 30
      }
    }
  }
}
